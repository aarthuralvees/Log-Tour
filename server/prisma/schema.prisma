generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String

  trips    Trip[]
}

model Trip {
  id                 Int     @id @default(autoincrement())
  userId             Int
  informacoesGerais  Json
  roteiroSugerido    Json

  user User @relation(fields: [userId], references: [id])
}

